{
  "Storage": {
    "ScreenshotsDirectory": "./TestData/Screenshots",
    "ThumbnailsDirectory": "./TestData/Thumbnails",
    "CreateDirectoriesIfNotExist": true,
    "MaxFileSizeBytes": 52428800,
    "DefaultOptimization": {
      "MaxWidth": 1920,
      "MaxHeight": 1080,
      "JpegQuality": 85,
      "GenerateThumbnail": true,
      "ThumbnailSize": {
        "Width": 300,
        "Height": 200
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Storage": "Debug"
    }
  }
}